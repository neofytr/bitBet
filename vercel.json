{
    "version": 2,
    "builds": [
      {
        "src": "api/server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/server.py"
      }
    ],
    "env": {
      "BITBETS_USERS": "",
      "BITBETS_GUESSES": "",
      "BITBETS_RESULTS": ""
    },
    "functions": {
      "api/server.py": {
        "maxDuration": 30
      }
    }
  }